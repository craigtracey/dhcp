# File managed by Chef

<% @allows.each do |allow| -%>
allow <%= allow %>;
<% end -%>

<% @parameters.each_pair do |param,value| -%>
<%= param %> <%= value %>;
<% end -%>

<% @options.each_pair do |option,value| -%>
option <%= option %> <%= value %>;
<% end -%>

include "<%= node[:dhcp][:config_dir] %>/subnets.d/subnet_list.conf";
